<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.schoolexchange.www.entity">
    <class name="BrowseRecords" table="browse_records">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="userId" column="user_id" type="java.lang.Integer"
                  not-null="true"/>
        <property name="goodsId" column="goods_id" type="java.lang.Integer"
                  not-null="true"/>
    </class>
</hibernate-mapping>