<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.schoolexchange.www.entity">
       <class name="Collection" table="collection">
              <id name="id" column="id">
                     <generator class="identity"/>
              </id>
              <property name="goods_id" column="goods_id" type="java.lang.Integer"
                        not-null="true"/>
              <property name="user_id" column="user_id" type="java.lang.Integer"
                        not-null="true"/>
       </class>
</hibernate-mapping>
